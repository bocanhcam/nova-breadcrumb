{
    "name": "bocanhcam/nova-breadcrumb",
    "description": "A Laravel Nova package provides an easy way to define and manage breadcrumbs.",
    "keywords": [
        "laravel",
        "nova",
        "breadcrumbs",
        "breadcrumb"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Bocanhcam\\NovaBreadcrumb\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Bocanhcam\\NovaBreadcrumb\\BreadcrumbServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
